#:kivy 1.0.9

<Element>:
    canvas: 
        Color: 
            rgb: 0.85, 0.62, 0.09
        Rectangle: 
            pos: self.pos
            size: 75, 125
        
    Label: 
        color: (0.46, 0.42, 0.03)
        font_size: 15
        top: root.top + 65
        x: 0
        text: "Helium"

<PeriodicTable>:
    element: helium

    canvas:
        Color: 
            rgb: 0.91, 0.91, 0.91
        Rectangle:
            pos: 0, 0
            size: self.width, self.height

    Element:
        id: helium
        top: root.top - 50
        x: 25